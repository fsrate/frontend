{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "product": {
      "type": "string"
    },
    "kind": {
      "type": "string"
    },
    "amount": {
      "type": "float"
    },
    "price": {
      "type": "float"
    },
    "total": {
      "type": "float"
    },
    "status": {
      "type": "string"
    },
    "upload": {
      "type": "component",
      "repeatable": false,
      "component": "users.user-order-uploads"
    },
    "category": {
      "via": "orders",
      "model": "category"
    },
    "user": {
      "via": "orders",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
